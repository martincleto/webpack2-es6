{
  "name": "webpack-es6",
  "version": "0.1.0",
  "author": "Santiago Mart√≠n-Cleto <martincleto@gmail.com>",
  "description": "Webpack 2 and ES6 boilerplate",
  "repository": "https://github.com/martincleto/webpack2-es6",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --config config/webpack.config.js --progress --colors",
    "watch": "webpack --config config/webpack.config.js --watch",
    "start": "webpack-dev-server --config config/webpack.config.js --hot --inline",
    "lint:js": "./node_modules/eslint/bin/eslint.js --cache -c config/.eslintrc.json --cache-location config/.eslintcache src/js/**/*.js test/js/**/*.js; exit 0",
    "lint:sass": "./node_modules/sass-lint/bin/sass-lint.js -c config/.sass-lint.yml 'src/sass/**/*.sass' -v -q",
    "lint": "npm run lint:sass && npm run lint:js",
    "test": "npm run lint:js && karma start config/karma.conf.js"
  },
  "keywords": [
    "webpack",
    "es6"
  ],
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-polyfill": "^6.20.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.13.1",
    "jasmine": "^2.5.3",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.1",
    "sass-lint": "^1.10.2",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
